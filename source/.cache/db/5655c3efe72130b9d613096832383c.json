ing: grayscale;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  display: inline-block;\n  margin: 0;\n}\n\n// Mixins\n\n// Config\n\n// Components\n\n// Pages\n@import 'pages/home';\n","src/assets/scss/pages/home.scss":".home {\n  display: block;\n  position: absolute;\n\n  width: 100%;\n  height: 100vh;\n\n  background-image: url('./../images/starry_sky.jpg');\n  background-color: rgba(0, 155, 255, 0.3);\n\n  &__bubbles {\n    width: 90%;\n    max-width: 430px;\n\n    margin: 10vh auto 0;\n  }\n\n  &__bubble {\n    position: relative;\n    display: block;\n\n    opacity: 1;\n\n    margin: 7px 0;\n    padding: 0.8em 1em;\n\n    background-color: white;\n\n    border-radius: 20px;\n\n    font-size: 14px;\n    font-family: \"Helvetica Neue\", \"Helvetica\", \"Arial\", sans-serif;\n    line-height: 21px;\n\n    box-shadow: 0 0 5px 1px rgba(0, 155, 255, 0.3);\n\n    a {\n      color: rgba(0, 150, 200, 1);\n\n      text-decoration: none;\n\n      border-bottom: 1px dotted rgba(0, 150, 200, 1);\n\n      &:active\n      &:visited {\n        color: rgba(0, 150, 200, 1);\n        border-color: rgba(0, 150, 200, 0.5);\n      }\n\n      &:hover {\n        color: rgba(0, 150, 200, 0.5);\n        border-color: rgba(0, 150, 200, 0.5);\n      }\n\n      &:focus {\n        outline: 1px dotted rgba(0, 150, 200, 0.5);\n      }\n    }\n\n    &__tail {\n      display: block;\n      position: absolute;\n      left: -10px;\n      bottom: 0;\n\n      width: 20px;\n      height: 20px;\n\n      background-image: url('./../images/bubble-tail.svg');\n\n      animation-duration: 2.35s;\n      animation-name: fadeOut;\n      animation-timing-function: ease-in;\n\n      &--active {\n        animation-name: none;\n      }\n\n      &--inactive {\n        opacity: 0;\n      }\n    }\n  }\n}\n\n@keyframes fadeOut {\n  0% {\n    opacity: 1;\n  }\n  85% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n\n@keyframes fadeIn {\n  0% {\n    opacity: 0;\n    transform: translate(-20px, 40px) scale(0.9);\n  }\n  100% {\n    opacity: 1;\n    transform: translate(0, 0) scale(1);\n  }\n}\n\n.fadeIn {\n  animation-duration: 0.35s;\n  animation-name: fadeIn;\n  animation-timing-function: ease-in-out;\n}\n\n"},"lineCount":null}},"error":null,"hash":"0560eca8f5c39e312ea89c69bc26b7e4","cacheData":{"env":{}}}