{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:vue/strongly-recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "key-spacing": 0,
    "quotes": [
      2,
      "single"
    ],
    "strict": 0,
    "semi": 2,
    "no-console": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-body-style": [
      1,
      "as-needed"
    ],
    "eqeqeq": 0,
    "class-methods-use-this": 0,
    "prefer-template": 1,
    "no-mixed-operators": 0,
    "one-var": 0,
    "no-multi-spaces": 1,
    "no-param-reassign": 1,
    "indent": [
      "error",
      2,
      {
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        },
      "SwitchCase": 1
      }
    ],
    "prefer-destructuring": ["error", {
      "array": false,
      "object": true
    }],
    "vue/html-closing-bracket-newline": ["error", {
      "singleline": "never",
      "multiline": "never"
    }],
    "vue/require-prop-types": false
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  }
}
